<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Secuencia con subsecuencias por aÃ±o -->
    <record id="seq_sale_order_custom" model="ir.sequence">
        <field name="name">Sales Order Custom</field>
        <field name="code">sale.order.custom</field>
        <field name="prefix">OV-%(year)s-</field>
        <field name="padding">4</field>
        <field name="number_next_actual">25</field>
        <field name="number_increment">1</field>
        <field name="use_date_range">True</field>
        <field name="implementation">standard</field>
        <field name="active">True</field>
    </record>
</odoo>


