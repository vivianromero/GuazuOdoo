                                                <th width="8%" class="text-center">Entradas</th>
                                                <th width="8%" class="text-center">Salidas</th>
                                                <th width="8%" class="text-center">Existencia Final</th>
                                            </tr>
											
                                            <tr>
                                                <th width="8%" class="text-center">Cantidad</th>
