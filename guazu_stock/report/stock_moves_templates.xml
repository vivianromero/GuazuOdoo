                                                <td class="text-right"><span t-esc="move['existence']"/></td>
                                                <td t-if="line['access_price']" class="text-right"><span t-esc="move['balance']"/></td>
                                                <td class="text-center"><span t-esc="move['from_to_location']"/></td>
                                            </tr>
